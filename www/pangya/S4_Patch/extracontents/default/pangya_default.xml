<?xml version="1.0" standalone="yes" ?>

<!-- definitions
##################################
   element    : <file>, <normal>, <over>, <selected>, <blink>
   attribute  : 
               name    : [must]   filename                                      ex) name="pangya.jpg"
               url     : [option] default "./" (current folder)                 ex) url="http://10.10.10.10:8888/extracontents/"
               version : [option] if specify, filename must be "name.version"   ex) name="pangya.jpg" version="1" 
                                                                                    The name of a file in a folder : "pangya.jpg.1"	
##################################
    element   : <weblink>
    attribute :
               url    : target url  ex) url="http://pangya.gametree.co.kr"	
    child     : <normal>, <over>, <selected>, <bllink>
##################################
	element   : <image>
	child     : <file>, <normal>, <over>, <selected>, <bllink>
##################################
	element   : <event>
	attribute :
               name   : event name that supplied from ntreev   ex) cardpack3rd, pointshop
        child     : <normal>, <over>, <selected>, <bllink>
##################################
	element   : <wallpaper>
	attribute :
	           prob    : relative probability (positive integer) ex) prob="10"
        child     : <file>
##################################
    element   : <topicons>
    child     : <weblink>, <image>, <event>
//////////////////////////
    element   : <lobby>
    child     : <wallpaper>
##################################
    element   : <loading>
    child     : <wallpaper>
##################################
    element   : <replacement>
    child     : <file>
##################################
-->

