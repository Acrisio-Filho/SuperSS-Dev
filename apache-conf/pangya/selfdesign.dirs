# Diretorio selfdesign
<Directory "${DOCROOT}/SelfDesign/">
    
    RewriteEngine On
	RewriteCond %{HTTP_HOST} !-d
	RewriteRule ^$ https://${VHNAME}:${VHPORT}/pangya.php [L,R=301]

    ErrorDocument 404 "Erro arquivo nao existe"
    ErrorDocument 403 "https://${VHNAME}:${VHPORT}/pangya.php"

    <FilesMatch ".*">
        require all denied
    </FilesMatch>

    <Files "UploadFiles.php">
        require all granted
    </Files>

 </Directory>